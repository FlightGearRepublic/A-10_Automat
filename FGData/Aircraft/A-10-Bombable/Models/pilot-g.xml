<?xml version="1.0"?>

<!-- pilot-g -->

<PropertyList>

 <path>A-10-accel.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>accelerations/pilot-g</property>
  <interpolation>
   <entry><ind>-5</ind><dep>-202.5</dep></entry>
   <entry><ind>-4</ind><dep>-112.5</dep></entry>
   <entry><ind>-3</ind><dep>-90</dep></entry>
   <entry><ind>-1</ind><dep>-45</dep></entry>
   <entry><ind>0</ind><dep>-22.5</dep></entry>
   <entry><ind>1</ind><dep>0</dep></entry>
   <entry><ind>3</ind><dep>45</dep></entry>
   <entry><ind>5</ind><dep>90</dep></entry>
   <entry><ind>7</ind><dep>135</dep></entry>
   <entry><ind>9</ind><dep>180</dep></entry>
   <entry><ind>10</ind><dep>202.5</dep></entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center> 
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
  <animation>
   <object-name>face</object-name> 
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
        <red>0.50</red>
        <green>0.25</green>
        <blue>0.18</blue>
      </emission>
  </animation>
  <animation>
   <object-name>needle-w</object-name> 
   <type>material</type>
    <emission> 
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
        <red>0.75</red>
        <green>0.55</green>
        <blue>0.35</blue>
      </emission>
  </animation>
 


<!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>
</PropertyList>
