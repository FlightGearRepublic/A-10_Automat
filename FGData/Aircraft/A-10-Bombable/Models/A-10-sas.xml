<?xml version="1.0"?>


<PropertyList>

	<path>A-10-sas.ac</path>

	<!-- Pitch SAS switch -->
	<animation>
		<object-name>pitch-L-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/sas/pitch-sas-switch</property>
		<factor>60</factor>
		<center>
			<x-m>-0.0213</x-m>
			<y-m>0.0178</y-m>
			<z-m>0.0000</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<object-name>pitch-R-switch</object-name>
		<type>rotate</type>
		<property>sim/model/A-10/controls/sas/pitch-sas-switch</property>
		<factor>60</factor>
		<center>
			<x-m>-0.0213</x-m>
			<y-m>0.0537</y-m>
			<z-m>0.0000</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>


	<!-- lighting -->

	<animation>
		<object-name>sas</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.385</red>
			<green>0.168</green>
			<blue>0.07</blue>
		</emission>
	</animation>


	<!--<animation> 
		<object-name>to-trim-light-on</object-name>
		<type>material</type>
		<emission> 
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
	</animation>-->

	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
